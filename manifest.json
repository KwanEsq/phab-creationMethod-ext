{
  "description": "Extended phabricator diff info",
  "manifest_version": 2,
  "name": "moz-phab-diff-info",
  "version": "0.2.0",
  "content_scripts": [
    {
      "matches": [
        "https://phabricator.services.mozilla.com/*",
        "https://phabricator-dev.allizom.org/*",
        "https://phab.mercurial-scm.org/*"
      ],
      "js": ["content.js"]
    }
  ]
}
